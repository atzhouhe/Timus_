using System;
namespace Judge1000
{
    
    class Excute
    {
        static void Main(string[] args)
        {
            string[] ss = Console.ReadLine().Split();
            Console.WriteLine(long.Parse(ss[0]) + long.Parse(ss[1]));
            Console.ReadLine();
        }
    }
}

