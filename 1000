using System;
namespace judge1000
{
     class Excute
     {
            Int a;
            int b;
            a = Convert.ToInt32(Console.ReadLine());
            b = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine( a + b);
            Console.ReadLine( );
      }
}
